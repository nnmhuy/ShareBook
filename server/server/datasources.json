{
  "db": {
    "host": "${DB_HOST}",
    "port": "${DB_PORT}",
    "url": "${DB_URL}",
    "database": "${DB_NAME}",
    "password": "${DB_PASSWORD}",
    "name": "db",
    "connector": "mongodb",
    "user": "${DB_USERNAME}",
    "useNewUrlParser": true
  },
  "imagestorage": {
    "name": "imagestorage",
    "connector": "loopback-component-storage",
    "maxFileSize": "800000",
    "allowedContentTypes": [
      "image/jpg",
      "image/jpeg",
      "image/png"
    ],
    "provider": "filesystem",
    "root": "./image-storage"
  },
  "cloudstorage": {
    "provider": "google",
    "keyFilename": "./key/sharebook-258403-bce550ef542f.json",
    "projectId": "sharebook-258403",
    "nameConflict": "makeUnique"
  }
}
