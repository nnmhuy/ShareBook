{
  "name": "user",
  "plural": "users",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": false
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true,
      "default": "New User"
    },
    "fbLink": {
      "type": "string"
    },
    "avatar": {
      "type": "string"
    },
    "phoneNumber": {
      "type": "string"
    },
    "coin": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "contribution": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "role": {
      "type": "string",
      "required": true,
      "enum": [
        "admin",
        "seller",
        "superAdmin",
        "stationStaff",
        "clubOwner",
        "clubAdmin",
        "clubStaff",
        "normalUser",
        "inactive"
      ],
      "default": "inactive"
    }
  },
  "validations": [],
  "relations": {
    "accessTokens": {
      "type": "hasMany",
      "model": "accessToken",
      "foreignKey": "userId"
    },
    "identities": {
      "type": "hasMany",
      "model": "userIdentity",
      "foreignKey": "userId"
    },
    "credentials": {
      "type": "hasMany",
      "model": "userCredential",
      "foreignKey": "userId"
    },
    "homeLocations": {
      "type": "belongsTo",
      "model": "location",
      "foreignKey": "homeLocationId",
      "options": {
        "nestRemoting": true
      }
    },
    "club": {
      "type": "belongsTo",
      "model": "club",
      "foreignKey": "clubId",
      "options": {
        "nestRemoting": true
      }
    },
    "likeBooks": {
      "type": "hasMany",
      "model": "likeBook",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true
      }
    },
    "likeReviews": {
      "type": "hasMany",
      "model": "likeReview",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true
      }
    },
    "likeReplies": {
      "type": "hasMany",
      "model": "likeReply",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true
      }
    },
    "logUserRoles": {
      "type": "hasMany",
      "model": "logUserRole",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true
      }
    },
    "joinClubRequests": {
      "type": "hasMany",
      "model": "joinClubRequest",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true
      }
    },
    "notifications": {
      "type": "hasMany",
      "model": "notification",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true
      }
    },
    "userLocations": {
      "type": "hasMany",
      "model": "userLocation",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true
      }
    },
    "bookmarks": {
      "type": "hasMany",
      "model": "bookmark",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "superAdmin",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "superAdmin",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
