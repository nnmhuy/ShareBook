{
  "name": "bookmark",
  "plural": "bookmarks",
  "base": "PersistedModel",
  "idInjection": true,
  "indexes": {
    "composeId": {
      "keys": {
        "userId": 1,
        "bookId": 1
      },
      "options": {
        "unique": true
      }
    }
  },
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": false
    },
    "ReadOnly": {
      "id": true
    }
  },
  "properties": {
    "isActive": {
      "type": "boolean",
      "required": true,
      "default": true
    }
  },
  "validations": [],
  "relations": {
    "book": {
      "type": "belongsTo",
      "model": "book",
      "foreignKey": "bookId"
    },
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "superAdmin",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "superAdmin",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "DENY",
      "property": "destroyById"
    }
  ],
  "methods": {}
}
