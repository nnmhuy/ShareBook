{
  "name": "transaction",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "holderType": {
      "type": "string",
      "required": true,
      "enum": [
        "singleUser",
        "club",
        "station"
      ]
    },
    "status": {
      "type": "string",
      "required": true,
      "enum": [
        "waitingForResponse",
        "waitingForTake",
        "isReading",
        "isOvertime",
        "waitingForDeadlineExtended",
        "deadlineExtended",
        "isReported",
        "isPaidDeposit",
        "isDone",
        "isCancel"
      ]
    },
    "isReviewed": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "passingDate": {
      "type": "date"
    },
    "returnDate": {
      "type": "date"
    },
    "extendedDeadline": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "note": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
