{
  "name": "transaction",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": false
    },
    "ReadOnly": {
      "id": true,
      "status": true
    }
  },
  "properties": {
    "holderType": {
      "type": "string",
      "required": true,
      "enum": [
        "singleUser",
        "club",
        "station"
      ],
      "default": "singleUser"
    },
    "status": {
      "type": "string",
      "required": true,
      "enum": [
        "waitingForResponse",
        "waitingForTake",
        "isReading",
        "isOvertime",
        "waitingForDeadlineExtended",
        "deadlineExtended",
        "isReported",
        "isPaidDeposit",
        "isDone",
        "isCancel"
      ],
      "default": "waitingForResponse"
    },
    "isReviewed": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "passingDate": {
      "type": "date"
    },
    "returnDate": {
      "type": "date"
    },
    "extendedDeadline": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "note": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "bookInstance": {
      "type": "belongsTo",
      "model": "bookInstance",
      "foreignKey": "bookInstanceId",
      "options": {
        "nestRemoting": true
      }
    },
    "borrower": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "borrowerId",
      "options": {
        "nestRemoting": true
      }
    },
    "holder": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "holderId",
      "options": {
        "nestRemoting": true
      }
    },
    "messages": {
      "type": "hasMany",
      "model": "messageInTransaction",
      "foreignKey": "transactionId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "superAdmin",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "superAdmin",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "DENY",
      "property": "destroyById"
    }
  ],
  "methods": {}
}
