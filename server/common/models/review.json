{
  "name": "review",
  "plural": "reviews",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": false
    }
  },
  "properties": {
    "facebookPostId": {
      "type": "string"
    },
    "numberOfLike": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "numberOfFbLike": {
      "type": "number"
    },
    "numberOfFbShare": {
      "type": "number"
    },
    "content": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "book": {
      "type": "belongsTo",
      "model": "book",
      "foreignKey": "bookId",
      "options": {
        "nestRemoting": true
      }
    },
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true
      }
    },
    "likeReviews": {
      "type": "hasMany",
      "model": "likeReview",
      "foreignKey": "reviewId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "superAdmin",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
