{
  "name": "review",
  "plural": "reviews",
  "base": "PersistedModel",
  "idInjection": true,
  "indexes": {
    "composeId": {
      "keys": {
        "userId": 1,
        "bookId": 1
      },
      "options": {
        "unique": true
      }
    }
  },
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": false
    },
    "ReadOnly": {
      "numberOfFbLike": true,
      "numberOfFbShare": true,
      "numberOfDislike": true,
      "numberOfLike": true,
      "id": true
    }
  },
  "properties": {
    "facebookPostId": {
      "type": "string"
    },
    "rating": {
      "type": "number",
      "required": true
    },
    "numberOfDislike": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "numberOfLike": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "numberOfFbLike": {
      "type": "number"
    },
    "numberOfFbShare": {
      "type": "number"
    },
    "content": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "book": {
      "type": "belongsTo",
      "model": "book",
      "foreignKey": "bookId",
      "options": {
        "nestRemoting": true
      }
    },
    "user": {
      "type": "belongsTo",
      "model": "user"
    },
    "likeReviews": {
      "type": "hasMany",
      "model": "likeReview",
      "foreignKey": "reviewId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "superAdmin",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "DENY",
      "property": "destroyById"
    }
  ],
  "methods": {}
}
