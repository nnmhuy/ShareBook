{
  "name": "report",
  "plural": "reports",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": false
    },
    "ReadOnly": {
      "id": true
    }
  },
  "properties": {
    "typeOfTarget": {
      "type": "string",
      "required": true,
      "enum": [
        "book",
        "bookInstance",
        "review",
        "reply",
        "user",
        "qa",
        "other"
      ]
    },
    "status": {
      "type": "string",
      "required": true,
      "enum": [
        "pending",
        "received",
        "resolved",
        "rejected"
      ]
    },
    "content": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "book": {
      "type": "belongsTo",
      "model": "book",
      "foreignKey": "bookId"
    },
    "bookInstance": {
      "type": "belongsTo",
      "model": "bookInstance",
      "foreignKey": "bookInstanceId"
    },
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId"
    },
    "reply": {
      "type": "belongsTo",
      "model": "reply",
      "foreignKey": "replyId"
    },
    "review": {
      "type": "belongsTo",
      "model": "review",
      "foreignKey": "reviewId"
    },
    "reporter": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "reporterId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "superAdmin",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "superAdmin",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "DENY",
      "property": "destroyById"
    }
  ],
  "methods": {}
}
