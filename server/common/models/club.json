{
  "name": "club",
  "plural": "clubs",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": false
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "logo": {
      "type": "string",
      "required": true
    },
    "isVerified": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "email": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "coin": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "contribution": {
      "type": "number",
      "required": true,
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "locations": {
      "type": "belongsTo",
      "model": "location",
      "foreignKey": "locationId",
      "options": {
        "nestRemoting": true
      }
    },
    "logUserRoles": {
      "type": "hasMany",
      "model": "logUserRole",
      "foreignKey": "clubId",
      "options": {
        "nestRemoting": true
      }
    },
    "joinClubRequests": {
      "type": "hasMany",
      "model": "joinClubRequest",
      "foreignKey": "clubId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "superAdmin",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "superAdmin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
