{
  "name": "book",
  "plural": "books",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "volume": {
      "type": "number",
      "required": true,
      "default": -1
    },
    "author": {
      "type": "string",
      "required": true,
      "default": "from internet"
    },
    "publisher": {
      "type": "string",
      "required": true
    },
    "publishYear": {
      "type": "date"
    },
    "description": {
      "type": "string"
    },
    "price": {
      "type": "number"
    },
    "numberOfPages": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "rating": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "numberOfUse": {
      "type": "number",
      "required": true,
      "default": 0
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
