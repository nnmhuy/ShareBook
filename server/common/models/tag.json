{
  "name": "tag",
  "plural": "tags",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": false
    },
    "ReadOnly": {
      "numberOfBook": true,
      "id": true
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "numberOfBook": {
      "type": "number",
      "require": true,
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "bookTags": {
      "type": "hasMany",
      "model": "bookTag",
      "foreignKey": "tagId",
      "options": {
        "nestRemoting": true
      }
    },
    "category": {
      "type": "belongsTo",
      "model": "category",
      "foreignKey": "categoryId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "superAdmin",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "superAdmin",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    }
  ],
  "methods": {}
}
