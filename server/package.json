{
  "name": "server",
  "version": "1.0.0",
  "main": "server/server.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "eslint .",
    "fix": "eslint . --fix",
    "start": "node .",
    "dev": "nodemon .",
    "posttest": "npm run lint && npm audit",
    "generate-key": "openssl genrsa -out server/private/localhost.key.pem 2048",
    "generate-cert": "openssl req -config server/localhost.conf -new -x509 -sha256 -nodes -key server/private/localhost.key.pem -days 365 -out server/private/localhost.cert.pem",
    "generate-csr": "openssl req -config server/localhost.conf -new -sha256 -nodes -key server/private/localhost.key.pem -days 365 -out server/private/localhost.req.pem",
    "print-cert": "openssl x509 -in server/private/localhost.cert.pem -text -noout",
    "print-csr": "openssl req -in server/private/localhost.req.pem -text -noout"
  },
  "dependencies": {
    "compression": "^1.0.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.5.2",
    "dotenv": "^8.0.0",
    "express-flash": "0.0.2",
    "express-session": "^1.16.2",
    "helmet": "^3.10.0",
    "lodash": "^4.17.15",
    "loopback": "^3.22.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^6.2.0",
    "loopback-component-passport": "^3.11.0",
    "loopback-component-storage": "^3.6.3",
    "loopback-connector-mongodb": "^5.0.0",
    "loopback-ds-readonly-mixin": "^2.0.4",
    "loopback-ds-timestamp-mixin": "^3.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth": "^2.0.0",
    "passport-local": "^1.0.0",
    "serve-favicon": "^2.0.1",
    "spdy": "^4.0.1",
    "strong-error-handler": "^3.0.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "eslint": "^3.17.1",
    "eslint-config-loopback": "^8.0.0"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "license": "UNLICENSED",
  "description": "ShareBookServer"
}
